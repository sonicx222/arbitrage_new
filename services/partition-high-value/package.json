{
  "name": "@arbitrage/partition-high-value",
  "version": "1.0.0",
  "description": "P3 High-Value Partition Detector Service (Ethereum, zkSync, Linea)",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "scripts": {
    "build": "tsc",
    "start": "cross-env PARTITION_ID=high-value node dist/index.js",
    "dev": "cross-env PARTITION_ID=high-value ts-node src/index.ts",
    "test": "jest",
    "test:unit": "jest --testPathPattern='__tests__/unit'",
    "test:integration": "jest --testPathPattern='__tests__/integration'",
    "test:coverage": "jest --coverage",
    "lint": "eslint src/**/*.ts",
    "typecheck": "tsc --noEmit"
  },
  "keywords": [
    "arbitrage",
    "partition",
    "high-value",
    "ethereum",
    "zksync",
    "linea",
    "defi"
  ],
  "author": "Arbitrage Team",
  "license": "MIT",
  "dependencies": {
    "@arbitrage/config": "*",
    "@arbitrage/core": "*",
    "@arbitrage/types": "*",
    "@arbitrage/unified-detector": "*"
  },
  "devDependencies": {
    "@types/jest": "^29.5.0",
    "@types/node": "^20.10.0",
    "cross-env": "^7.0.3",
    "jest": "^29.7.0",
    "ts-jest": "^29.1.0",
    "ts-node": "^10.9.2",
    "typescript": "^5.6.2"
  },
  "engines": {
    "node": ">=22.0.0"
  }
}
