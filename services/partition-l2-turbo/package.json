    "test": "jest",
    "typecheck": "tsc --noEmit",
    "lint": "eslint src/ --ext .ts"
  },
  "dependencies": {
    "@arbitrage/unified-detector": "*",
    "@arbitrage/core": "*",
    "@arbitrage/config": "*",
    "@arbitrage/types": "*"
  },
  "devDependencies": {
    "@types/jest": "^29.5.0",
    "@types/node": "^20.0.0",
    "cross-env": "^7.0.3",
    "jest": "^29.7.0",
    "rimraf": "^6.0.1",
    "ts-jest": "^29.1.0",
    "ts-node": "^10.9.0",
    "typescript": "^5.6.2"
  },
  "engines": {
    "node": ">=22.0.0"
  },
  "keywords": [
    "arbitrage",
    "partition",
    "l2-turbo",
    "arbitrum",
    "optimism",
    "base",
    "ethereum-l2"
